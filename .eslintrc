{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier/standard"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "max-len": ["error", { "code": 140 }],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}

